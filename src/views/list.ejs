<!DOCTYPE html>
<html lang="pt-br">
<head>
    <%- include('partials/head.ejs') %>
</head>
<body>
    
    <h1><%= title %></h1>

    <%- include('partials/navbar.ejs') %>

    

    <ul>
        <% users.forEach((user) => { %>
            <li>
                <%= user.name %> - <%= user.email %> - Idade: <%= user.age %>
                <a href="/edit?id=<%= user._id %>">[editar]</a>
                <a href="/remove/<%= user._id %>">[remover]</a>
            </li>
        <% }) %>

    </ul>

    <script src="js/main.js"></script>
</body>
</html>